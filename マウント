const wrapper = mount(Component)
//              â†“
//         ã€Vueå†…éƒ¨å‡¦ç†ã€‘
//              â†“
//        1. beforeCreate
//              â†“  
//        2. created      â† APIå‘¼ã³å‡ºã—ãªã©
//              â†“
