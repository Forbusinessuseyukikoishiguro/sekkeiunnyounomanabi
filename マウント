  const spy = jest.spyOn(Component.methods, 'fetchData') // APIç›£è¦–
  
  const wrapper = mount(Component)                     // ãƒã‚¦ãƒ³ãƒˆï¼ˆmountedãŒå®Ÿè¡Œã•ã‚Œã‚‹ï¼‰
  
  expect(spy).toHaveBeenCalled()                      // APIå‘¼ã³å‡ºã—ç¢ºèª
})

it('created ã§åˆæœŸåŒ–ã•ã‚Œã‚‹', () => {
  const wrapper = mount(Component, {                   // ãƒã‚¦ãƒ³ãƒˆ
    propsData: { userId: 123 }                        // ãƒ—ãƒ­ãƒ‘ãƒ†ã‚£è¨­å®š
  })
  
  // created ã§è¨­å®šã•ã‚ŒãŸãƒ‡ãƒ¼ã‚¿ã‚’ç¢ºèª
  expect(wrapper.vm.isInitialized).toBe(true)
})
```

